{
  "bootstrap": "vendor/autoload.php",
  "path": "performance",
  "progress": "dots",
  "time_unit": "microseconds",
  "retry_threshold": 10,
  "reports": {
    "with-params": {
      "extends": "aggregate",
      "cols": ["benchmark", "groups", "subject", "mean", "params"]
    },
    "simple": {
      "extends": "aggregate",
      "cols": ["benchmark", "groups", "subject", "mean"]
    }
  },
  "outputs": {
    "all": {
      "extends": "html",
      "file": "report.html",
      "title": "Jose Performance Test Suite"
    },
    "md": {
      "extends": "markdown",
      "file": "report.md",
      "title": "Jose Performance Test Suite"
    }
  }
}
